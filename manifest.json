{
  "manifest_version": 2,
  // "update_url":"http://clients2.google.com/service/update2/crx",
  "name": "AActive ClearJC",
  "description": "This extension active ClearJC.",
  "version": "1.0",

  "permissions": [
    "tabs", "http://*/*", "https://*/*", "activeTab", "storage"
  ],

  "browser_action": {
    "default_title": "ClearJC",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }

